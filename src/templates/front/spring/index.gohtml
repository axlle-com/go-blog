                            <form method="post" action="index.html">
                                <div class="form-group">
                                    <input type="text" name="fullname" value="" placeholder="Full Name" required>
                                </div>

                                <div class="form-group">
                                    <input type="text" name="email" value="" placeholder="Your Email" required>
                                </div>

                                <div class="form-group">
                                    <textarea name="message" placeholder="Message" rows="4" required></textarea>
                                </div>

                                <div class="form-group">
                                    <button type="submit" class="theme-btn btn-style-two">Submit <span class="icon ti-arrow-circle-right"></span></button>
                                </div>

                            </form>
