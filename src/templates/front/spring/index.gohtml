    <!--Contact Form Section-->
    <section class="contact-page-section" id="contact">
        <div class="auto-container">
            <div class="upper-section">
                <div class="row clearfix">
                    <!--Form Column-->
                    <div class="col-md-6 col-sm-12 col-xs-12">
